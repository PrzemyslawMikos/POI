<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PoiBundle\Entity\Changes" table="changes">
    <indexes>
      <index name="FK_Changes_Points_idx" columns="Point_Id"/>
      <index name="FK_Changes_Versions_idx" columns="Version_Id"/>
    </indexes>
    <id name="id" type="integer" column="Id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="date" type="datetime" column="Date" nullable="false"/>
    <field name="actiontype" type="integer" column="ActionType" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="point" target-entity="Points" fetch="LAZY">
      <join-columns>
        <join-column name="Point_Id" referenced-column-name="Id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="version" target-entity="Versions" fetch="LAZY">
      <join-columns>
        <join-column name="Version_Id" referenced-column-name="Id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

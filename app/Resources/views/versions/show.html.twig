{% extends 'dashboard.html.twig' %}

{% block content %}
    <h1>Versions</h1>

    <table>
        <tbody>
            <tr>
                <th>Members</th>
                <td>{{ version.members }}</td>
            </tr>
            <tr>
                <th>Addeddate</th>
                <td>{% if version.addeddate %}{{ version.addeddate|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ version.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('versions_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('versions_edit', { 'id': version.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}

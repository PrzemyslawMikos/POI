                            </li>
                        </ul>
                        <!-- /.nav-second-level -->
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-institution fa-fw"></i> Types<span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level">
                            <li>
                                <a href="{{ path('types_index') }}"><i class="fa fa-list fa-fw"></i> View all</a>
                            </li>
                            <li>
                                <a href="{{ path('types_new') }}"><i class="fa fa-plus fa-fw"></i> Add</a>
                            </li>
                        </ul>
                        <!-- /.nav-second-level -->
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-star-half-o fa-fw"></i> Ratings<span class="fa arrow"></span></a>

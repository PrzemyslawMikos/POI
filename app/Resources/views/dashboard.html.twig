                            <li>
                                <a href="{{ path('ratings_index') }}"><i class="fa fa-list fa-fw"></i> View all</a>
                            </li>
                            <li>
                                <a href="panels-wells.html"><i class="fa fa-star fa-fw"></i> Best rated</a>
                            </li>
                            <li>
                                <a href="panels-wells.html"><i class="fa fa-level-up fa-fw"></i> Most rated</a>
                            </li>
                        </ul>
                        <!-- /.nav-second-level -->
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-users fa-fw"></i> Users<span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level">
                            <li>
                                <a href="#"><i class="fa fa-android fa-fw"></i> Android application<span class="fa arrow"></span></a>
                                <ul class="nav nav-third-level">
                                    <li>
                                        <a href="{{ path('users_index') }}"><i class="fa fa-list fa-fw"></i> View all</a>
                                    </li>
                                    <li>
                                        <a href="#"><i class="fa fa-exclamation-triangle fa-fw"></i> Warnings</a>
                                    </li>
                                    <li>

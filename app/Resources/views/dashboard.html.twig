                                <a href="#"><i class="fa fa-user fa-fw"></i> Admin<span class="fa arrow"></span></a>
                                <ul class="nav nav-third-level">
                                    <li>
                                        <a href="{{ path('administrators_index') }}"><i class="fa fa-list fa-fw"></i> View all</a>
                                    </li>
                                    <li>
                                        <a href="{{ path('registration_admin') }}"><i class="fa fa-plus-circle fa-fw"></i> Register new</a>
